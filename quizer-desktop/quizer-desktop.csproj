<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>quizer_desktop</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Resources\quizer-icon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\quizer-icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\quizer-icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\quizer-icon.ico" />
  </ItemGroup>

</Project>
